;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Doge The Creeps"
run/main_scene="res://project/src/scene/main/Main.tscn"
config/features=PackedStringArray("4.2")
boot_splash/bg_color=Color(1, 0.972549, 0.862745, 1)
